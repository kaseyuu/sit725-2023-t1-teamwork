        integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous" />

    <script type="module">
        import Autocomplete from
            "https://cdn.jsdelivr.net/gh/lekoala/bootstrap5-autocomplete@master/autocomplete.js";
        var src = []; for (let i = 0; i < 50; i++) {
            src.push({
                title: "Option " +
                    i, id: "opt" + i, data: { key: i, },
            });
        }
        Autocomplete.init("input.autocomplete", {
            items: src, valueField: "id",
            labelField: "title", highlightTyped: true, onSelectItem: console.log,
        });
    </script>
</head>

